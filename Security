var password = prompt("Daily access token required.");
if(password != 'Jakel'){
document.body.innerHTML = '';
document.body.innerHTML = 'You have been denied access';
function stateChange(newState) {
    setTimeout(function () {
        if (newState == -1) {
            window.close()
        }
    }, 5000);
}
}else{
alert('Access Granted.');
}
